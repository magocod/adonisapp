{
  "name": "adonisapp",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "node ace build --production",
    "start": "node server.js",
    "dev": "node ace serve --watch",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write .",
    "test": "node -r @adonisjs/assembler/build/register japaFile.ts",
    "coverage": "nyc npm run test",
    "test_functional": "node -r @adonisjs/assembler/build/register japaFile.ts test/functional/**/*.spec.ts",
    "test_auth": "node -r @adonisjs/assembler/build/register japaFile.ts test/functional/auth/**/*.spec.ts",
    "test_role": "node -r @adonisjs/assembler/build/register japaFile.ts test/functional/role/**/*.spec.ts",
    "test_user": "node -r @adonisjs/assembler/build/register japaFile.ts test/functional/user/**/*.spec.ts",
    "test_unit": "node -r @adonisjs/assembler/build/register japaFile.ts test/unit/**/*.spec.ts",
    "test_unique": "node -r @adonisjs/assembler/build/register japaFile.ts test/functional/user/delete_user.spec.ts"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^5.3.1",
    "@types/jsdom": "^16.2.10",
    "@types/supertest": "^2.0.11",
    "adonis-preset-ts": "^2.1.0",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-adonis": "^1.3.2",
    "eslint-plugin-prettier": "^3.4.0",
    "execa": "^5.0.0",
    "get-port": "^5.1.1",
    "japa": "^3.1.1",
    "jsdom": "^16.5.3",
    "nyc": "^15.1.0",
    "pino-pretty": "^5.0.2",
    "prettier": "^2.3.1",
    "supertest": "^6.1.3",
    "typescript": "~4.2",
    "youch": "^2.2.2",
    "youch-terminal": "^1.1.1"
  },
  "dependencies": {
    "@adonisjs/auth": "^8.0.9",
    "@adonisjs/core": "^5.3.2",
    "@adonisjs/lucid": "^16.0.0",
    "@adonisjs/repl": "^3.1.6",
    "@adonisjs/view": "^6.1.0",
    "@types/socket.io": "^2.1.13",
    "luxon": "^1.26.0",
    "mysql": "^2.18.1",
    "pg": "^8.6.0",
    "phc-argon2": "^1.1.1",
    "proxy-addr": "^2.0.6",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^4.0.2",
    "source-map-support": "^0.5.19",
    "sqlite3": "^5.0.2"
  }
}
